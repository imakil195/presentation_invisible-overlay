{
  "name": "invisible-presenter-overlay",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "build:mac": "tsc && vite build && electron-builder --mac",
    "build:win": "tsc && vite build && electron-builder --win --x64",
    "build:all": "tsc && vite build && electron-builder -mw",
    "preview": "vite preview"
  },
  "dependencies": {
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "wait-on": "^9.0.3"
  },
  "description": "**Version:** 1.1.0-MVP **Target Platforms:** Windows 10/11, macOS 12+ (Monterey or newer) **Framework:** Electron (React + TypeScript)",
  "main": "dist-electron/main.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "build": {
    "appId": "com.invisible.overlay",
    "productName": "Invisible Overlay",
    "directories": {
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg"
      ],
      "identity": null
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "guid": "5e5j-invisible-overlay-v1",
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ]
  }
}